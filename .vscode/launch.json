{
    "configurations": [
        {
            "name": "smallweb up",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "up",
                "--addr=:7777",
                "--ssh-addr=:2222"
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/example",
            },
        },
        {
            "name": "smallweb.run up",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "up",
                "--addr=:7777",
                "--ssh-addr=:2222"
            ],
            "env": {
                "SMALLWEB_DIR": "${userHome}/smallweb/smallweb.run",
                "SMALLWEB_DOMAIN": "smallweb.localhost"
            },
        },
        {
            "name": "smallweb cli",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "console": "integratedTerminal",
            "args": [
                // your args here
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/example",
            },
        },
        {
            "name": "opentelemetry",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "up",
                "--addr=:7777",
                "--ssh-addr=:2222"
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/example",
                "OTEL_DENO": "true",
            }
        }
    ]
}
