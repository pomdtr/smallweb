        },
        {
            "name": "smallweb cli",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "console": "integratedTerminal",
            "args": [
                "doctor"
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/cmd/embed/smallweb",
                "SMALLWEB_DOMAIN": "localhost",
            },
        }
    ]
}
