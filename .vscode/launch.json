{
    "configurations": [
        {
            "name": "smallweb up",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "up",
                "--addr=:7777",
                "--ssh-addr=:2222"
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/examples",
            },
        },
        {
            "name": "smallweb cli",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "console": "integratedTerminal",
            "args": [
                // your args here
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/examples",
            },
        },
        {
            "name": "opentelemetry",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "up",
                "--addr=:7777",
                "--ssh-addr=:2222"
            ],
            "env": {
                "SMALLWEB_DIR": "${workspaceFolder}/examples",
                "OTEL_DENO": "true"
            }
        }
    ]
}
